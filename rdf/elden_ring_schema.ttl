@prefix er: <http://www.semanticweb.org/fall2025/eldenring/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

er: a owl:Ontology ;
    rdfs:comment "Comprehensive Elden Ring Ontology: Production Gameplay & Full Lore"@en .

er:Ammo rdfs:subClassOf er:Item .

er:AshOfWar rdfs:subClassOf er:Item .

er:BellBearing rdfs:subClassOf er:Item .

er:Castle rdfs:subClassOf er:Location .

er:Catacombs rdfs:subClassOf er:Location .

er:Cave rdfs:subClassOf er:Location .

er:Consumable rdfs:subClassOf er:Item .

er:Cookbook rdfs:subClassOf er:Item .

er:Creature rdfs:subClassOf er:Agent .

er:CrystalTear rdfs:subClassOf er:Item .

er:Demigod rdfs:subClassOf er:Agent .

er:Dungeon rdfs:subClassOf er:Location .

er:Empyrean rdfs:subClassOf er:Agent .

er:Equippable a owl:Class ;
    owl:unionOf ( er:Weapon er:Armor er:Talisman er:Shield ) .

er:Evergaol rdfs:subClassOf er:Location .

er:Family a owl:Class .

er:Fort rdfs:subClassOf er:Location .

er:GreatRune rdfs:subClassOf er:Item .

er:HolyWeapon a owl:Class ;
    owl:intersectionOf ( er:Weapon [ a owl:Restriction ;
                owl:onProperty er:requiresFaith ;
                owl:someValuesFrom xsd:integer ] ) .

er:Incantation rdfs:subClassOf er:Spell .

er:KeyItem rdfs:subClassOf er:Item .

er:Material rdfs:subClassOf er:Item .

er:PeacefulEntity a owl:Class ;
    owl:intersectionOf (
        er:Agent
        [ a owl:Class ; owl:complementOf er:Boss ]
    ) .

er:Region rdfs:subClassOf er:Location .

er:Remembrance rdfs:subClassOf er:Item .

er:Ruins rdfs:subClassOf er:Location .

er:SiteOfGrace rdfs:subClassOf er:Location .

er:Sorcery rdfs:subClassOf er:Spell .

er:SpiritAsh rdfs:subClassOf er:Item .

er:Tool rdfs:subClassOf er:Item .

er:Tower rdfs:subClassOf er:Location .

er:Whetblade rdfs:subClassOf er:Item .

er:attackFire a owl:DatatypeProperty ;
    rdfs:range xsd:float .

er:attackHoly a owl:DatatypeProperty ;
    rdfs:range xsd:float .

er:attackLightning a owl:DatatypeProperty ;
    rdfs:range xsd:float .

er:attackMagic a owl:DatatypeProperty ;
    rdfs:range xsd:float .

er:attackPhysical a owl:DatatypeProperty ;
    rdfs:range xsd:float .

er:causesEffect a owl:ObjectProperty ;
    rdfs:domain er:Weapon ;
    rdfs:range er:StatusEffect .

er:childOf a owl:ObjectProperty ;
    rdfs:domain er:Agent ;
    rdfs:range er:Agent ;
    owl:inverseOf er:parentOf .

er:fpCost a owl:DatatypeProperty ;
    rdfs:range xsd:integer .

er:gameId a owl:DatatypeProperty,
        owl:InverseFunctionalProperty ;
    rdfs:range xsd:string .

er:hasMaxStats a owl:FunctionalProperty,
        owl:ObjectProperty ;
    rdfs:domain er:Weapon ;
    rdfs:range er:StatBlock .

er:hasSkill a owl:ObjectProperty ;
    rdfs:domain er:Weapon ;
    rdfs:range er:Skill ;
    owl:inverseOf er:skillOf .

er:hp a owl:DatatypeProperty ;
    rdfs:range xsd:string .

er:hpCost a owl:DatatypeProperty ;
    rdfs:range xsd:integer .

er:locatedIn a owl:ObjectProperty,
        owl:TransitiveProperty ;
    rdfs:range er:Location .

er:memberOf a owl:ObjectProperty ;
    rdfs:domain er:Agent ;
    rdfs:range er:Faction ;
    owl:inverseOf er:hasMember .

er:mentions a owl:ObjectProperty ;
    owl:inverseOf er:mentionedIn .

er:participatedIn a owl:ObjectProperty ;
    rdfs:domain er:Agent ;
    rdfs:range er:Event ;
    owl:inverseOf er:hasParticipant .

er:requiresArcane a owl:DatatypeProperty ;
    rdfs:range xsd:integer .

er:requiresDexterity a owl:DatatypeProperty ;
    rdfs:range xsd:integer .

er:requiresIntelligence a owl:DatatypeProperty ;
    rdfs:range xsd:integer .

er:requiresStrength a owl:DatatypeProperty ;
    rdfs:range xsd:integer .

er:scalingArcane a owl:DatatypeProperty ;
    rdfs:range xsd:string .

er:scalingDexterity a owl:DatatypeProperty ;
    rdfs:range xsd:string .

er:scalingFaith a owl:DatatypeProperty ;
    rdfs:range xsd:string .

er:scalingIntelligence a owl:DatatypeProperty ;
    rdfs:range xsd:string .

er:scalingStrength a owl:DatatypeProperty ;
    rdfs:range xsd:string .

er:sells a owl:ObjectProperty ;
    rdfs:domain er:Merchant ;
    rdfs:range er:Item ;
    owl:inverseOf er:soldBy .

er:siblingOf a owl:ObjectProperty,
        owl:SymmetricProperty ;
    rdfs:domain er:Agent ;
    rdfs:range er:Agent .

er:spouseOf a owl:ObjectProperty,
        owl:SymmetricProperty ;
    rdfs:domain er:Agent ;
    rdfs:range er:Agent .

er:staminaCost a owl:DatatypeProperty ;
    rdfs:range xsd:integer .

er:teaches a owl:ObjectProperty ;
    rdfs:domain er:NPC ;
    rdfs:range er:Spell ;
    owl:inverseOf er:taughtBy .

er:value a owl:DatatypeProperty ;
    rdfs:range xsd:integer .

er:weight a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:range xsd:float .

er:worships a owl:ObjectProperty ;
    rdfs:domain er:Agent ;
    rdfs:range er:OuterGod ;
    owl:inverseOf er:worshippedBy .

er:Armor rdfs:subClassOf er:Item .

er:Boss rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty er:drops ],
        er:Agent .

er:Fire a er:DamageType,
        owl:NamedIndividual .

er:Holy a er:DamageType,
        owl:NamedIndividual .

er:Lightning a er:DamageType,
        owl:NamedIndividual .

er:Magic a er:DamageType,
        owl:NamedIndividual .

er:Merchant rdfs:subClassOf er:NPC .

er:OuterGod a owl:Class .

er:Physical a er:DamageType,
        owl:NamedIndividual .

er:Shield rdfs:subClassOf er:Item .

er:Skill a owl:Class .

er:StatBlock a owl:Class .

er:StatusEffect a owl:Class .

er:Talisman rdfs:subClassOf er:Item .

er:droppedBy a owl:ObjectProperty .

er:drops a owl:ObjectProperty ;
    rdfs:domain er:Agent ;
    rdfs:range er:Item ;
    owl:inverseOf er:droppedBy .

er:hasMember a owl:ObjectProperty .

er:hasParticipant a owl:ObjectProperty .

er:mentionedIn a owl:ObjectProperty .

er:parentOf a owl:ObjectProperty .

er:requiresFaith a owl:DatatypeProperty ;
    rdfs:range xsd:integer .

er:skillOf a owl:ObjectProperty .

er:soldBy a owl:ObjectProperty .

er:taughtBy a owl:ObjectProperty .

er:worshippedBy a owl:ObjectProperty .

er:NPC rdfs:subClassOf er:Agent .

er:Spell rdfs:subClassOf er:Item .

er:Event a owl:Class ;
    owl:disjointWith er:Concept,
        er:Faction .

er:Concept a owl:Class .

er:DamageType a owl:Class ;
    owl:oneOf ( er:Physical er:Magic er:Fire er:Lightning er:Holy ) .

er:Faction a owl:Class ;
    owl:disjointWith er:Concept .

er:Weapon rdfs:subClassOf er:Item .

er:Location a owl:Class ;
    owl:disjointWith er:Concept,
        er:Event,
        er:Faction .

er:Agent a owl:Class ;
    owl:disjointWith er:Concept,
        er:Event,
        er:Faction,
        er:Item,
        er:Location .

er:Item a owl:Class ;
    owl:disjointWith er:Concept,
        er:Event,
        er:Faction,
        er:Location .

