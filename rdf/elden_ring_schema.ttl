@prefix er: <http://www.semanticweb.org/fall2025/eldenring/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

er: a owl:Ontology ;
    rdfs:comment "The Official Elden Ring Knowledge Graph Ontology (DLC Updated)"@en .

er:Ammo rdfs:subClassOf er:Item .

er:AshOfWar rdfs:subClassOf er:Item .

er:Axe rdfs:subClassOf er:Weapon .

er:BackhandBlade rdfs:subClassOf er:Weapon .

er:Ballista rdfs:subClassOf er:Weapon .

er:BeastClaw rdfs:subClassOf er:Weapon .

er:BellBearing rdfs:subClassOf er:Item .

er:Bow rdfs:subClassOf er:Weapon .

er:ChestArmor rdfs:subClassOf er:Armor .

er:Claw rdfs:subClassOf er:Weapon .

er:ColossalSword rdfs:subClassOf er:Weapon .

er:Concept a owl:Class .

er:Consumable rdfs:subClassOf er:Item .

er:Cookbook rdfs:subClassOf er:Item .

er:Crossbow rdfs:subClassOf er:Weapon .

er:CurvedGreatsword rdfs:subClassOf er:Weapon .

er:CurvedSword rdfs:subClassOf er:Weapon .

