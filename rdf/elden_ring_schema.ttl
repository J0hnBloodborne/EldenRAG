er:HeavyThrustingSword rdfs:subClassOf er:Weapon .

er:Helm rdfs:subClassOf er:Armor .

er:Incantation rdfs:subClassOf er:Item,
        er:Magic .

er:Katana rdfs:subClassOf er:Weapon .

er:KeyItem rdfs:subClassOf er:Item .

er:LegArmor rdfs:subClassOf er:Armor .

er:LightBow rdfs:subClassOf er:Weapon .

er:LightGreatsword rdfs:subClassOf er:Weapon .

er:Material rdfs:subClassOf er:Item .

er:PerfumeBottle rdfs:subClassOf er:Weapon .

er:Reaper rdfs:subClassOf er:Weapon .

er:SacredSeal rdfs:subClassOf er:Weapon .

er:Shield rdfs:subClassOf er:Item .

er:Sorcery rdfs:subClassOf er:Item,
        er:Magic .

er:Spear rdfs:subClassOf er:Weapon .

er:SpiritAsh rdfs:subClassOf er:Item .

er:StraightSword rdfs:subClassOf er:Weapon .

er:Talisman rdfs:subClassOf er:Item .

er:ThrowingBlade rdfs:subClassOf er:Weapon .

er:ThrustingSword rdfs:subClassOf er:Weapon .

er:Tool rdfs:subClassOf er:Item .

er:Torch rdfs:subClassOf er:Weapon .

er:Twinblade rdfs:subClassOf er:Weapon .

er:Whetblade rdfs:subClassOf er:Item .

er:Whip rdfs:subClassOf er:Weapon .

er:attackFire a owl:DatatypeProperty ;
    rdfs:domain er:WeaponStats ;
    rdfs:range xsd:float .

er:attackHoly a owl:DatatypeProperty ;
    rdfs:domain er:WeaponStats ;
    rdfs:range xsd:float .

er:attackLightning a owl:DatatypeProperty ;
    rdfs:domain er:WeaponStats ;
    rdfs:range xsd:float .

er:attackMagic a owl:DatatypeProperty ;
    rdfs:domain er:WeaponStats ;
    rdfs:range xsd:float .

er:attackPhysical a owl:DatatypeProperty ;
    rdfs:domain er:WeaponStats ;
    rdfs:range xsd:float .

er:attackStamina a owl:DatatypeProperty ;
    rdfs:domain er:WeaponStats ;
    rdfs:range xsd:float .

er:causesEffect a owl:ObjectProperty ;
    rdfs:domain er:Weapon ;
    rdfs:range er:StatusEffect .

er:description a owl:DatatypeProperty ;
    rdfs:range xsd:string .

er:drops a owl:ObjectProperty ;
    rdfs:comment "What an agent drops on death." ;
    rdfs:domain er:Agent ;
    rdfs:range er:Item ;
    owl:inverseOf er:droppedBy .

er:foundIn a owl:ObjectProperty ;
    rdfs:comment "Where an item can be looted." ;
    rdfs:domain er:Item ;
    rdfs:range er:Location .

er:hasMaxStats a owl:ObjectProperty ;
    rdfs:comment "Links a Weapon to its stats at Max Upgrade level." ;
    rdfs:domain er:Weapon ;
    rdfs:range er:WeaponStats .

er:hasSkill a owl:ObjectProperty ;
    rdfs:domain er:Weapon ;
    rdfs:range er:Skill .

er:healthPoints a owl:DatatypeProperty ;
    rdfs:domain er:Agent ;
    rdfs:range xsd:integer .

er:locatedIn a owl:ObjectProperty ;
    rdfs:comment "Where an entity can be found." ;
    rdfs:domain er:Agent ;
    rdfs:range er:Location .

er:mentions a owl:ObjectProperty ;
    rdfs:comment "Generic lore connection found in text descriptions." .

er:requiresArcane a owl:DatatypeProperty ;
    rdfs:domain er:Weapon ;
    rdfs:range xsd:integer .

er:requiresDexterity a owl:DatatypeProperty ;
    rdfs:domain er:Weapon ;
    rdfs:range xsd:integer .

er:requiresFaith a owl:DatatypeProperty ;
    rdfs:domain er:Weapon ;
    rdfs:range xsd:integer .

er:requiresIntelligence a owl:DatatypeProperty ;
    rdfs:domain er:Weapon ;
    rdfs:range xsd:integer .

er:requiresStrength a owl:DatatypeProperty ;
    rdfs:domain er:Weapon ;
    rdfs:range xsd:integer .

er:runesDropped a owl:DatatypeProperty ;
    rdfs:domain er:Boss ;
    rdfs:range xsd:integer .

er:scalingArcane a owl:DatatypeProperty ;
    rdfs:domain er:WeaponStats ;
    rdfs:range xsd:string .

er:scalingDexterity a owl:DatatypeProperty ;
    rdfs:domain er:WeaponStats ;
    rdfs:range xsd:string .

er:scalingFaith a owl:DatatypeProperty ;
    rdfs:domain er:WeaponStats ;
    rdfs:range xsd:string .

er:scalingIntelligence a owl:DatatypeProperty ;
    rdfs:domain er:WeaponStats ;
    rdfs:range xsd:string .

er:scalingStrength a owl:DatatypeProperty ;
    rdfs:domain er:WeaponStats ;
    rdfs:range xsd:string .

er:sells a owl:ObjectProperty ;
    rdfs:domain er:Merchant ;
    rdfs:range er:Item .

er:unlocksReward a owl:ObjectProperty ;
    rdfs:domain er:Remembrance ;
    rdfs:range er:Item .

er:weight a owl:DatatypeProperty ;
    rdfs:domain er:Item ;
    rdfs:range xsd:float .

er:Boss rdfs:subClassOf er:Agent .

er:Merchant rdfs:subClassOf er:NPC .

er:NPC rdfs:subClassOf er:Agent .

er:Remembrance rdfs:subClassOf er:Item .

er:StatusEffect a owl:Class .

er:droppedBy a owl:ObjectProperty ;
    rdfs:comment "Inverse of drops." ;
    rdfs:domain er:Item ;
    rdfs:range er:Agent .

er:Location a owl:Class .

er:Magic a owl:Class .

er:Armor rdfs:subClassOf er:Item .

er:Agent a owl:Class .

er:WeaponStats a owl:Class ;
    rdfs:comment "A helper node containing the stats of a weapon at a specific upgrade level." .

er:Item a owl:Class .

er:Weapon rdfs:subClassOf er:Item .

