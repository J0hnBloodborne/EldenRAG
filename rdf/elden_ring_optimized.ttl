er:ScholarsShield a er:Sorcery,
        er:Spell ;
    rdfs:label "Scholar's Shield" ;
    ns1:image "http://eldenring.wiki.fextralife.com/file/Elden-Ring/scholars_shield-elden-ring-wiki-guide-200px.png" ;
    rdfs:comment "One of the glintstone sorceries of the Academy of Raya Lucaria. Enchants shield held in the left hand with additional damage negation.This sorcery can be cast while in motion. Taught to the Knights of the Cuckoo by the academy as payment for their contract." .

er:SchoolOfGravenMages a er:Agent,
        er:Creature ;
    rdfs:label "School of Graven Mages" ;
    ns1:image "http://eldenring.wiki.fextralife.com/file/Elden-Ring/school_of_graven_mages_enemies_elden_ring_wiki_600px.jpg" .

