    ns1:image "http://eldenring.wiki.fextralife.com/file/Elden-Ring/flame_chariots_enemies_elden_ring_wiki_600px.jpg" .

er:FlameCleanseMe a er:Incantation,
        er:Spell ;
    rdfs:label "Flame, Cleanse Me" ;
