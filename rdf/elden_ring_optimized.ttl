er:MapRauhRuins a er:Item,
        er:KeyItem ;
    rdfs:label "Map: Rauh Ruins" ;
    ns1:image "http://eldenring.wiki.fextralife.com/file/Elden-Ring/map_rauh_ruins_map_elden_ring_shadow_of_the_erdtree_dlc_wiki_guide_200px.png" ;
    rdfs:comment "Map of the ancient Rauh Ruins. These giant ruins are flooded, overgrown, and split into two by a chasm. To the southwest is the Church of the Bud, overlooking the tower of shadow. " .

er:MapScaduAltus a er:Item,
        er:KeyItem ;
