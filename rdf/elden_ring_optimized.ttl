        er:Weapon ;
    rdfs:label "Pata" ;
    ns1:image "http://eldenring.wiki.fextralife.com/file/Elden-Ring/pata_elden_ring_shadow_of_the_erdtree_dlc_wiki_guide_200px.png" ;
